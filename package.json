    "flux": "^3.1.3",
    "form-serialize": "^0.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.13",
    "mdi-material-ui": "^5.7.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "path": "^0.12.7",
    "react": "^16.4.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.4.2",
    "react-file-drop": "^0.2.8",
