    "@babel/preset-react": "^7.0.0",
    "aws-sdk": "^2.289.0",
    "babel-loader": "^8.0.6",
    "child_process": "^1.0.2",
    "core-js": "^3.1.4",
    "fs": "0.0.1-security",
    "indent-string": "2.1.0",
